<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('style') }}
        {% endblock %}
    </head>
    <body >
        <header>
            {% include 'Common/nav.html.twig' %}
        </header>
        {% block body %}
        {% endblock %}


        {% block footer %}
            {{ include('Common/footer.html.twig') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
